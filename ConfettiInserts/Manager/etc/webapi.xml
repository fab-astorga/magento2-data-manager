<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="POST" url="/V1/confetti_inserts/import">
        <service class="ConfettiInserts\Manager\Api\ConfettiInsertsManagementInterface" method="importConfettiInsertsFromCsv"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/confetti_inserts">
        <service class="ConfettiInserts\Manager\Api\ConfettiInsertsManagementInterface" method="getConfettiInserts"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>